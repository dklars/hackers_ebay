{% extends 'base.html.twig' %}

{% block title %}HackersFor{% endblock %}

{% block body %}
    <h1>HackersFor</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ hackers_for.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ hackers_for.name }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ hackers_for.mail }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ hackers_for.nombre }}</td>
            </tr>
            <tr>
                <th>Activated</th>
                <td>{{ hackers_for.activated ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_hackers_for_index') }}">back to list</a>

    <a href="{{ path('app_hackers_for_edit', {'id': hackers_for.id}) }}">edit</a>

    {{ include('hackers_for/_delete_form.html.twig') }}
{% endblock %}
